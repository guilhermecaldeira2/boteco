{
  "name": "boteco",
  "version": "2.0.1",
  "description": "Fastest framework for develop Botmaker Bots, but with low changes can use with different chatbot hosts;",
  "main": "./bin/index.js",
  "scripts": {
    "build": "babel src --extensions \" .js,.ts\" --out-dir bin --copy-files --no-copy-ignored",
    "build:prd": "npm run build && npm run emitDeclaration",
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules src/Server.ts",
    "emitDeclaration": "tsc --emitDeclarationOnly",
    "start": "node ./bin/index.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "BotMaker",
    "WhatsApp",
    "ChatBot"
  ],
  "author": "",
  "license": "ISC"
}
